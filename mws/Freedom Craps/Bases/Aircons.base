views:
  - type: table
    name: Age
    filters:
      and:
        - file.hasTag("airconditioner")
        - file.hasProperty("dob")
    order:
      - file.name
      - dob
    sort: []
  - type: table
    name: Identity
    filters:
      and:
        - file.hasTag("airconditioner")
        - file.hasProperty("Gender-Identity")
    order:
      - file.name
      - Gender-Identity
    sort:
      - property: Gender-Identity
        direction: ASC
