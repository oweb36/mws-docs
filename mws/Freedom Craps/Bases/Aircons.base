views:
  - type: table
    name: Age
    filters:
      and:
        - file.hasTag("airconditioner")
        - file.hasProperty("dob")
    order:
      - file.name
      - dob
    sort:
      - property: dob
        direction: ASC
  - type: table
    name: Identity
    filters:
      and:
        - file.hasTag("airconditioner")
